<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView android:id="@+id/Time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_red_dark"
        android:layout_weight="1"
        android:text="Time"/>

    <TextView android:id="@+id/MemFree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_red_dark"
        android:layout_weight="1"
        android:text="MemFree"/>

    <TextView android:id="@+id/Cached"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_red_dark"
        android:layout_weight="1"
        android:text="Cached"/>

    <TextView android:id="@+id/MediaMemIdle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_red_dark"
        android:layout_weight="1"
        android:text="MediaMemIdle" />

    <TextView android:id="@+id/RealFree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_red_dark"
        android:layout_weight="1"
        android:text="RealFree"/>

    <TextView android:id="@+id/AvaliMem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_red_dark"
        android:layout_weight="1"
        android:text="AvaliMem"/>

</LinearLayout>
